{
  "name": "caleydo_core",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_core",
  "caleydo": {
    "plugins": {
      "web": [
        {
          "type": "datatype",
          "id": "matrix",
          "file": "matrix_impl"
        },
        {
          "type": "datatype",
          "id": "table",
          "file": "table_impl"
        },
        {
          "type": "datatype",
          "id": "vector",
          "file": "vector_impl"
        },
        {
          "type": "datatype",
          "id": "stratification",
          "file": "stratification_impl"
        }
      ]
    },
    "dependencies": {
      "web": {
        "requirejs": "https://github.com/sgratzl/requirejs-bower.git#master",
        "require-css": "~0.1.8"
      },
      "tsd": {
        "es6-promise": "35119c"
      }
    },
    "requirejs-config": {
      "map": {
        "*": {
          "css": "${baseUrl}/require-css/css.js"
        }
      }
    },
    "requirejs-bower": {
      "ignore": [
        "requirejs",
        "require-css"
      ]
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
